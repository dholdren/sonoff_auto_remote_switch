{
  "configurations": [
    {
      "name": "ESP8266",
      "includePath": [
        "${workspaceFolder}/**",
        "${env:HOME}/.arduino15/packages/esp8266/hardware/esp8266/*/cores/esp8266",
        "${env:HOME}/.arduino15/packages/esp8266/hardware/esp8266/*/libraries/**",
        "${env:HOME}/Documents/Arduino/libraries/**",
        "${env:HOME}/.arduino15/packages/esp8266/hardware/esp8266/*/variants/generic"
      ],
      "defines": [
        "ESP8266",
        "ARDUINO=10819",
        "ARDUINO_ESP8266_GENERIC",
        "ARDUINO_ARCH_ESP8266",
        "F_CPU=80000000L",
        "F_FLASH=40000000L",
        "FLASH_MODE_DIO",
        "LWIP_OPEN_SRC"
      ],
      "compilerPath": "${env:HOME}/.arduino15/packages/esp8266/tools/xtensa-lx106-elf-gcc/*/bin/xtensa-lx106-elf-gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-x64"
    }
  ],
  "version": 4
}